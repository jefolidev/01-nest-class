{
  "extends": "@rocketseat/eslint-config/node",
  "rules": {
    "no-useless-constructor": "off",
    "no-unused-vars": "warn",
    "no-new": "off",
    "no-use-before-define": "off",
    "semi": "off",
    "prettier/prettier": [
      "off",
      {
        "semi": false,
        "singleQuote": true,
        "trailingComma": "all",
        "endOfLine": "auto"
      }
    ]
  }
}